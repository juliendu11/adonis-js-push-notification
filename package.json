{
  "name": "@juliendu11/adonis-js-push-notification",
  "description": "Enables the addition of support for sending FCM push notifications",
  "version": "0.0.3",
  "engines": {
    "node": ">=20.6.0"
  },
  "type": "module",
  "files": [
    "src",
    "providers",
    "index.ts",
    "configure.ts",
    "build"
  ],
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./push_notification_provider": "./build/providers/push_notification_provider.js",
    "./services/main": "./build/services/push_notification.js",
    "./types": "./build/src/types/main.js"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "del-cli build",
    "compile": "npm run clean && tsc",
    "copy:templates": "copyfiles \"stubs/**/**/*.stub\" build",
    "format": "prettier --write .",
    "lint": "eslint . --ext=.ts",
    "postcompile": "npm run copy:templates",
    "precompile": "npm run lint",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "quick:test": "node --enable-source-maps --loader=ts-node-maintained/esm bin/test.ts",
    "release": "npx release-it",
    "test": "c8 npm run quick:test",
    "typecheck": "tsc --noEmit",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juliendu11/adonis-js-push-notification"
  },
  "keywords": [
    "adonisjs",
    "push",
    "notifications",
    "service",
    "firebase",
    "fcm",
    "apns",
    "ios",
    "android"
  ],
  "author": "juliendu11",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.12.0",
    "@adonisjs/eslint-config": "2.0.0-beta.7",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.6.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^9.15.0",
    "fastify": "^5.6.2",
    "np": "^10.0.6",
    "prettier": "^3.3.2",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.0"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  }
}
